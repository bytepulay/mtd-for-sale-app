        <LinearLayout
            android:weightSum="10"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

          <EditText
                   android:layout_weight="5"
                    style="@style/Foredit"
                    android:id="@+id/gram"
                    android:background="@drawable/custom_edittext_form_background"
                    android:hint="Gram"
                    android:inputType="numberDecimal" />

          <Button
              android:background="@drawable/button_login_back"
              style="@style/Foredit"
              android:id="@+id/Add"
              android:textColor="#FFF"
              android:layout_weight="5"
              android:text="Add"/>

        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="10dp"
            android:background="#fafafa"
            android:weightSum="15"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <EditText
                style="@style/Foredit"
                android:id="@+id/kyat"
                android:layout_weight="5"
                android:background="@drawable/custom_edittext_form_background"
                android:hint="က်ပ္"
                android:inputType="number" />

            <EditText
                style="@style/Foredit"
                android:id="@+id/pal"
                android:layout_weight="5"
                android:background="@drawable/custom_edittext_form_background"
                android:hint="ပဲ"
                android:inputType="number"/>

            <EditText
                style="@style/Foredit"
                android:id="@+id/yae"
                android:layout_weight="5"
                android:background="@drawable/custom_edittext_form_background"
                android:hint=" ေရြး"
                android:inputType="number"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="10dp"
            android:background="#fafafa"
            android:weightSum="10"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <EditText
                style="@style/Foredit"
                android:id="@+id/custBuyNumber"
                android:layout_weight="5"
                android:background="@drawable/custom_edittext_form_background"
                android:hint="Qualtity"
                android:inputType="number"
                />

            <EditText
                style="@style/Foredit"
                android:id="@+id/custDiscountPoint"
                android:layout_weight="5"
                android:background="@drawable/custom_edittext_form_background"
                android:hint="Point Eight"
                android:inputType="number"
                />

        </LinearLayout>



        <LinearLayout
            android:layout_marginTop="10dp"
            android:background="#fafafa"
            android:weightSum="10"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <EditText
                style="@style/Foredit"
                android:id="@+id/aYot"
                android:layout_weight="5"
                android:background="@drawable/custom_edittext_form_background"
                android:hint="Ayot"
                android:inputType="number"
                />

            <EditText
                style="@style/Foredit"
                android:id="@+id/totalAyot"
                android:layout_weight="5"
                android:background="@drawable/custom_edittext_form_background"
                android:hint="Total Ayot"
                android:inputType="number"
                />

        </LinearLayout>




